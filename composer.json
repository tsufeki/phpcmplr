{
    "name": "tsufeki/phpcmplr",
    "description": "PHP auto-complete engine",
    "license": "GPL-3.0+",
    "type": "project",
    "authors": [
        {
            "name": "tsufeki",
            "email": "tsufeki@ymail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tsufeki/http.git"
        }
    ],
    "require": {
        "react/http": "dev-master",
        "tsufeki/react-line-stream": "dev-master",
        "tsufeki/react-filesystem-monitor": "dev-master",
        "tsufeki/php-lenient-parser": "dev-master",
        "justinrainbow/json-schema": "^2.0",
        "katzgrau/klogger": "^1.2",
        "composer/composer": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "PhpCmplr\\": "src/PhpCmplr/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\PhpCmplr\\": "tests/PhpCmplr/"
        }
    }
}
